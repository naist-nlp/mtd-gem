            }
        ]
    }, 
    {
        "text": "", 
        "line": 47, 
        "range": [], 
        "entities": []
    }, 
    {
        "text": "Day 2: Labuan Bajo \u2013 Rinca Island \u2013 Kanawa Island", 
        "line": 48, 
        "range": [
            4133, 
            4185
        ], 
        "entities": [
            {
                "type": "LOC_NAME", 
                "span": [
                    4154, 
                    4165
                ], 
