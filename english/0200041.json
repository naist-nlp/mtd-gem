            }, 
            {
                "type": "LOC_NAME", 
                "span": [
                    5876, 
                    5885
                ], 
                "name": "Loh Liang"
            }, 
            {
                "type": "LOC_NAME", 
                "span": [
                    5889, 
                    5902
                ], 
                "name": "Komodo Island"
            }, 
            {
                "type": "TRANS_NOM", 
                "span": [
                    5953, 
